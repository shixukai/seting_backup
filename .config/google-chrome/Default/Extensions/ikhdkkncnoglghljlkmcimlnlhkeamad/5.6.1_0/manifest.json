{
   "author": "Milk Lee( milk.lee@qq.com )",
   "background": {
      "scripts": [ "/js/lib/require.js", "/js/bg.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "/img/logo.png",
         "38": "/img/logo.png"
      },
      "default_popup": "popup.html",
      "default_title": "点击切换划词翻译开关"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Q",
            "mac": "MacCtrl+Q"
         }
      },
      "translate": {
         "description": "翻译网页中选中的文本",
         "suggested_key": {
            "default": "Alt+A"
         }
      },
      "web": {
         "description": "翻译整个网页",
         "suggested_key": {
            "default": "Alt+Z"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "/theme/selection.css" ],
      "js": [ "/js/lib/jquery.js", "/js/lib/doT.js", "/js/module/storage.js", "/js/select.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://code.jquery.com; object-src 'self'",
   "description": "支持谷歌、百度、有道、必应四大翻译和朗读引擎，可以方便的查看、复制和朗读不同引擎的翻译结果。",
