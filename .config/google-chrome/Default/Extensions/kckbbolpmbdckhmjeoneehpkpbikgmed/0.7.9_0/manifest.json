{
   "background": {
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "image/off_icon_19.png",
         "38": "image/off_icon_38.png"
      },
      "default_popup": "popup.html"
   },
   "default_locale": "en",
   "description": "Falcon Proxy for SSLedge user. Simple, clear and easy to use.",
   "icons": {
      "128": "image/icon_128.png",
      "24": "image/icon_24.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "SSLedge Falcon Proxy",
   "permissions": [ "\u003Call_urls>", "webRequest", "webRequestBlocking", "notifications", "proxy" ],
   "sandbox": {
      "pages": [ "sandbox.html" ]
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.7.9"
}
